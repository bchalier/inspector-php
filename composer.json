{
    "name": "log-engine/logengine-php",
    "description": "LOG-Engine package for PHP applications.",
    "keywords": ["apm", "php", "logengine"],
    "license": "MIT",
    "authors": [
        {
            "name": "Valerio Barbera",
            "email": "support@logengine.dev"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "psr/log": "^1.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-apache": "*"
    },
    "autoload": {
        "psr-4": {
            "LogEngine\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "psr-4": {
            "LogEngine\\Tests\\": "tests/"
        }
    }
}
